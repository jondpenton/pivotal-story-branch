{
  "name": "pivotal-story-branch",
  "description": "Pivotal Tracker Story Branch Generator is a tool to automatically generate a branch name for a new story in Pivotal Tracker.",
  "homepage": "https://github.com/jpenton/pivotal-story-branch",
  "version": "0.0.0-development",
  "main": "./dist/index.js",
  "bin": {
    "psb": "./bin/run"
  },
  "license": "MIT",
  "scripts": {
    "build": "rm -rf ./dist/ && tsc",
    "start": "node ./dist/index.js",
    "dev": "nodemon -e ts --exec \"yarn build && yarn start\"",
    "semantic-release": "semantic-release",
    "test": "jest",
    "test:ci": "jest --ci --coverage",
    "lint": "run-p lint:*",
    "lint:commit": "commitlint --from HEAD~1 --to HEAD --verbose",
    "lint:format": "prettier -c 'src/**/*.ts'"
  },
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.0",
    "axios": "^0.20.0",
    "dotenv": "^8.2.0",
    "ora": "^5.0.0",
    "slugify": "^1.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.7",
    "@types/axios": "^0.14.0",
    "@types/jest": "^26.0.13",
    "@types/node": "^14.6.1",
    "babel-jest": "^26.3.0",
    "chai": "^4.2.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "msw": "^0.20.5",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "oclif": "^1.16.1",
    "prettier": "^2.1.1",
    "pretty-quick": "^3.0.0",
    "semantic-release": "^17.1.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jpenton/pivotal-story-branch.git"
  },
  "oclif": {
    "bin": "psb",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/dist"
  ]
}
